<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Container Tracking API Credentials - Loaded Automatically on Module Install -->
    
    <!-- TimeToCargo API Key -->
    <record id="timetocargo_api_key" model="ir.config_parameter">
        <field name="key">timetocargo.api_key</field>
        <field name="value">6e54abb6-3ab2-47a9-9107-97139f66bac2</field>
    </record>

    <!-- TimeToCargo API Endpoint URL -->
    <record id="timetocargo_api_url" model="ir.config_parameter">
        <field name="key">timetocargo.api_url</field>
        <field name="value">https://tracking.timetocargo.com/v1/container</field>
    </record>

    <!-- API Timeout (seconds) -->
    <record id="timetocargo_api_timeout" model="ir.config_parameter">
        <field name="key">timetocargo.api_timeout</field>
        <field name="value">30</field>
    </record>
</odoo>
